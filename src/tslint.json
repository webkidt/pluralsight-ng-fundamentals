{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            [
                "app",
                "events",
                "event",
                "nav",
                "session",
                "create"
            ],
            "kebab-case"
        ]
    }
}
