{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            [
                "app",
                "modal",
                "validate"
            ],
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            [
                "app",
                "events",
                "event",
                "nav",
                "session",
                "create",
                "collapsible",
                "simple",
                "upvote"
            ],
            "kebab-case"
        ]
    }
}
